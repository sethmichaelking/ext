{
  "name": "Anywhere.io",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Treat every page like its a Google doc.",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "permissions": ["scripting"],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}
